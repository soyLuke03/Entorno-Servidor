<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Salida</title>
<link rel="stylesheet" href="cssPiramide.css">
</head>
<body>

<%String numberStr = request.getParameter("numero");
  int number = Integer.valueOf(numberStr); %>


<div align="center" id="imagen">
<%for(int i = 1;i<=number;i++){
	for(int o = 0;o<i;o++){%>
		
		<img align="center" id="imagen" height="50px" width="50" src="data:image/jpeg;base64,">
		
	<%}%>
	<br>
<%}%>

<%for(int i = -1;i>=number;i--){
	for(int o = 0;o<i;o++){%>
		
		<img align="center" id="imagen" height="50px" width="50" src="data:image/jpeg;base64,">
		
	<%}%>
	<br>
<%}%>
</div>
</body>
</html>